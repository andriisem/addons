<?xml version="1.0" encoding="utf-8"?>

<odoo>
    <template id="snippets" inherit_id="website.snippets">
        <xpath expr="//div[@id='snippet_feature']/div[hasclass('o_panel_body')]" position="inside">
            <t t-snippet="google_search.page_width"
               t-thumbnail="/google_search/static/src/img/icon.png" />
        </xpath>
    </template>

    <template id="page_width" name="Search Page Width">
        <section class="snippet_page_width mt32 mb32">
            <div class="container">
                <div class="row">
                    <div class="col-lg-12">
                        <div class="cse"></div>
                    </div>
                </div>
            </div>
        </section>
    </template>
</odoo>