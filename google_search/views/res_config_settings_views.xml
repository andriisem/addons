<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="res_config_settings_view_form" model="ir.ui.view">
        <field name="name">res.config.settings.view.form.inherit.google.search</field>
        <field name="model">res.config.settings</field>
        <field name="inherit_id" ref="base_setup.res_config_settings_view_form"/>
        <field name="arch" type="xml">
            <div id="webmaster_settings" position="after">
                <h2>Google Search</h2>
                <div class="row mt16 o_settings_container" id="">
                    <div class="col-12 col-lg-6 o_setting_box" id="">
                    <div class="o_setting_left_pane"/>
                        <div class="o_setting_right_pane">
                            <label for="google_search_id" string="Google Search ID"/>
                            <field name="google_search_id" nolabel="1"/>
                            <a href="https://developers.google.com/custom-search/?hl=RU" class="oe-link" target="_blank"><i class="fa fa-fw fa-arrow-right"/>Tutorial</a>
                        </div>
                    </div>
                </div>
            </div>
        </field>
    </record>
</odoo>